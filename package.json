{
	"name": "redacted-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsc --noEmit",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"start": "npm run build && npm ci --omit dev && node -r dotenv/config build"
	},
	"peerDependencies": {
		"@tensorflow/tfjs-converter": "^3.0.0",
		"@tensorflow/tfjs-core": "^3.0.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/kit": "^1.20.4",
		"@tensorflow-models/qna": "1.0.1",
		"@tensorflow/tfjs-backend-cpu": "^3.15.0",
		"@tensorflow/tfjs-converter": "^3.15.0",
		"@tensorflow/tfjs-core": "^3.15.0",
		"@testing-library/svelte": "^4.0.3",
		"@types/jsdom": "^21.1.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"jsdom": "^22.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.6",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"engines": {
		"node": ">=16",
		"npm": ">=8"
	},
	"dependencies": {
		"@vercel/kv": "0.2.2",
		"dotenv": "^16.3.1"
	}
}
